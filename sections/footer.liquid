<footer class="footer-unceil">
    <div class="footer-upper">
        <div class="footer-left text-left">
            <div class="footer-form">
                <p class="b-18-r m-b-16-r">{{section.settings.join}}</p>
                <div class="footer-form-block">
                    <div class="footer-form-input">
                        <input id="email" type="text" class="footer_email_input" name="contact[email]"
                            placeholder="{{ section.settings.email}}" required>
                        <button class="btn btn-black">
                            {{section.settings.btn}}
                        </button>
                    </div>
                    <div class="footer-form-checkbox">
                        <input type="checkbox" id="confirm" name="confirm" value="confirm">
                        <label class="b-16-r m-b-14-r" for="confirm">{{section.settings.confirm}}</label><br>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-right">
            <div class="footer-right-block m-o-1">
                <p class="allcaps">{{section.settings.footer_1}}</p>
                <div class="footer-menu-block">
                    {% for link in linklists.footer-1.links %}
                    <a class="b-14-r" class="b-14-r" href="{{link.url}}">{{link.title}}</a>
                    {% endfor %}
                </div>
            </div>
            <div class="footer-right-block m-o-3">
                <p class="allcaps">{{section.settings.footer_2}}</p>
                <div class="footer-menu-block">
                    {% for link in linklists.footer-2.links %}
                    <a class="b-14-r" href="{{link.url}}">{{link.title}}</a>
                    {% endfor %}
                </div>
            </div>
            <div class="footer-right-block m-o-2">
                <p class="allcaps">{{section.settings.footer_3}}</p>
                <div class="footer-menu-block">
                    <a class="b-14-r" href="{{settings.social_instagram_link}}">Instagram</a>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-bottom">

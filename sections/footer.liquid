        <div class="footer-btm">
            <p class="b-12-r text-left">{{section.settings.copyright}}</p>
            <div class="footer-right-bottom text-right m-m-0">
                <a class="b-12-r" href="{{section.settings.privacy_url}}">{{section.settings.privacy}}</a>
                <a class="b-12-r" href="{{section.settings.tos_url}}">{{section.settings.tos}}</a>
            </div>
        </div>
    </div>
</footer>

